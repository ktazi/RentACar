<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="RentACar.Controller.SupVehController"
      prefHeight="400.0" prefWidth="600.0"
alignment="TOP_CENTER">
    <padding>
        <Insets top="20" left="20" right="20" bottom="20"/>
    </padding>
    <Text styleClass="title" text="Suppression Vehicule"/>
    <VBox alignment="TOP_CENTER">
        <padding>
            <Insets top="30" left="20" right="20" bottom="20"/>
        </padding>
        <Text text="Valider la suppression du vehicule selectionne ?"/>
        <Button fx:id="button" text="Supprimer" onAction="#supVehi"/>
    </VBox>

</VBox>
